{
    "name": "stacktrace/laravel-translations",
    "description": "Opinionated translation package for Laravel.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "StackTrace\\Translations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Peter Stovka",
            "email": "ps@stacktrace.sk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "StackTrace\\Translations\\TranslationsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.17",
        "pestphp/pest": "^2.28"
    }
}
